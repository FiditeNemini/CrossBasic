#tag Events cmdRun
	#tag Event
		Sub Pressed()
		  Dim output As String = RunCrossBasic(txtCode.Text.ReplaceLineEndings(EndOfLine), chkDebug.Value).DefineEncoding(Encodings.UTF8)
		  txtOutput.Text = output
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events cmdRun1
	#tag Event
		Sub Pressed()
		  lstFunctions.RemoveAllRows()
		  lstClasses.RemoveAllRows()
		  
		  GetFunctions()
		  
		  for i as integer = 0 to CrossBasic.functions.LastIndex
		    var c as CrossBasic.PluginFunction = CrossBasic.functions(i)
		    
		    lstFunctions.AddRow( c.Name, c.Arity.ToString, c.Location )
		  next
		  
		  for i as integer = 0 to CrossBasic.classes.LastIndex
		    var c as CrossBasic.PluginClass = CrossBasic.classes(i)
		    
		    lstClasses.AddRow( c.Name, c.Location )
		  next
		End Sub
	#tag EndEvent
#tag EndEvents
